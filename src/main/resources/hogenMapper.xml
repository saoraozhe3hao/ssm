<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hogen.repository.CityMapper">
    <select id="getCity" parameterType="long" resultType="com.hogen.sqlResult.City">
        select id,name as cityName,countryCode from city where id= #{id}
    </select>
</mapper>